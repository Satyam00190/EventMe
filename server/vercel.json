
{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ,
    {
        "src": "/(.*)",
        "dest": "server.js"
    }
],
"routes": [
    {
        "src": "/(.*)",
        "dest": "/server.js"
    }
]
}